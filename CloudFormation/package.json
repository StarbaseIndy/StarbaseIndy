{
  "name": "github-scripting",
  "version": "1.0.0",
  "description": "POC for scripting against github API",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "docker-build-aws-sam": "docker build -t aws-sam-cli --target aws-sam -f ./docker/Dockerfile .",
    "docker-build-x11-vnc-ssh": "docker build -t x11-vnc-ssh --target x11-vnc-ssh -f ./docker/Dockerfile .",
    "docker-build": "npm run docker-build-aws-sam && npm run docker-build-x11-vnc-ssh",
    "sh": "node scripts/launch.js aws-sam-cli --entrypoint /bin/sh",
    "aws": "node scripts/launch.js aws-sam-cli --entrypoint aws",
    "sam": "node scripts/launch.js aws-sam-cli",
    "x11-vnc-ssh": "node scripts/launch.js x11-vnc-ssh",
    "bash": "node scripts/launch.js x11-vnc-ssh --entrypoint /bin/bash",
    "down": "docker-compose -f docker/docker-compose.yml down -v",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@octokit/rest": "^16.34.1",
    "cross-env": "^6.0.3",
    "csv": "^5.1.3",
    "octokat": "^0.10.0",
    "slash": "^3.0.0",
    "wreck": "^14.2.0"
  }
}
